{
  "name": "@zettel/site",
  "description": "Examples of using Zettel, works on codesandbox",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "SKIP_PREFLIGHT_CHECK=true ../node_modules/.bin/react-scripts start",
    "build": "SKIP_PREFLIGHT_CHECK=true ../node_modules/.bin/react-scripts build",
    "postbuild": "react-snap"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "concurrency": 1,
    "inlineCss": false,
    "skipThirdPartyRequests": false
  },
  "dependencies": {
    "@microsoft/fast-tooling-react": "^1.23.0",
    "@types/prismjs": "^1.16.1",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@types/react-router-dom": "^4.3.4",
    "@types/styled-components": "^4.1.22",
    "@types/typography": "^0.16.3",
    "@types/use-persisted-state": "^0.3.0",
    "@zettel/core": "^0.0.23",
    "@zettel/react": "^0.0.23",
    "changelog-parser": "^2.8.0",
    "lodash-es": "^4.17.15",
    "prismjs": "^1.21.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-draggable": "^4.0.3",
    "react-jsonschema-form": "^1.8.0",
    "react-router-dom": "^5.0.1",
    "react-text-selection-popover": "^1.3.2",
    "styled-components": "^4.4.1",
    "typography": "^0.16.19",
    "use-persisted-state": "^0.3.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "add": "^2.0.6",
    "mdx.macro": "^0.2.9",
    "puppeteer": "^3.1.0",
    "react-dnd-html5-backend": "^9.4.0",
    "react-movable": "^2.2.0",
    "react-snap": "1.10.0",
    "yarn": "^1.17.3"
  }
}
